{
  "expo": {
    "name": "AuxFit",
    "slug": "auxfit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconLogo.png",
    "scheme": "appfitness",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Splash-Screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#121316ff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Splash-Screen.png",
        "backgroundColor": "#121316ff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
